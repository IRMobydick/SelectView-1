<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SelectView">
        <attr name="orientation" format="enum">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr>
        <attr name="textSelectedColor" format="color"/>
        <attr name="selectedScaleType" format="enum">
            <enum name="centerCrop" value="0"/>
            <enum name="centerInside" value="1"/>
            <enum name="fitCenter" value="2"/>
            <enum name="fitXY" value="3"/>
        </attr>
        <attr name="selectedColor" format="color"/>
        <attr name="notSelectedColor" format="color"/>
        <attr name="fontSize" format="dimension"/>
        <attr name="selectedDrawable" format="reference"/>
    </declare-styleable>
</resources>